{
  "extends": ["react-app", "prettier"],
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "react-hooks/exhaustive-deps": "off",
        "printWidth": 120,
        "singleQuote": true,
        "tabWidth": 2,
        "useTabs": false,
        "arrowParens": "always",
        "trailingComma": "es5",
        "semi": true,
        "endOfLine": "auto"
      }
    ]
  }
}
